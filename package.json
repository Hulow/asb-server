{
  "scripts": {
    "build": "rm -rf dist && tsc",
    "start": "npm run build && cd dist/ && node src/index.js",
    "lint": "eslint --max-warnings=0 .",
    "lint:fix": "eslint --fix --max-warnings=0 .",
    "prepare": "husky install",
    "test:unit": "jest --testMatch='**/*.spec.ts' --testPathPattern",
    "test:intg": "jest --testMatch='**/*.intg-spec.ts' --runInBand --testPathPattern",
    "_typeorm": "ts-node ./node_modules/.bin/typeorm -d ./ormconfig.js",
    "mig:create": "npm run _typeorm migration:create",
    "mig:run": "npm run _typeorm migration:run -d",
    "mig:revert": "npm run _typeorm migration:revert",
    "mig:test:run": "NODE_ENV=test npm run _typeorm migration:run -d",
    "mig:test:revert": "NODE_ENV=test npm run _typeorm migration:revert"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "http-errors": "^2.0.0",
    "inversify": "^6.0.1",
    "pg": "^8.10.0",
    "pino": "^8.12.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.16",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/http-errors": "^2.0.1",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.16.0",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "axios": "^1.4.0",
    "eslint": "^8.39.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-extended": "^3.2.4",
    "lint-staged": "^13.2.1",
    "prettier": "^2.8.8",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "lint-staged": {
    "src/**/*.{ts,json,md}": [
      "prettier --write",
      "eslint --fix --max-warnings=0"
    ]
  }
}
